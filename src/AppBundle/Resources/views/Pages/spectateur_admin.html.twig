{% extends "@App/baseAdmin.html.twig" %}

{% block contenu %}Administrator

    <p>
        <a href="{{ path('accueil')}}">Lien accueil</a>
    </p>

    <div class="text-align-center">
        {% if spectateur %}
                <p>
                    spectateur : {{ spectateur.civilitespectateur}}
                    {{ spectateur.nomSpectateur}}
                    {{ spectateur.prenomSpectateur}}


                </p>
                <a href="{{ path('admin_supp_spectateur',  {'id': spectateur.id}) }}">
                    <button class="supprimer" type="submit">Supprimer</button>
                </a>
                <a href="{{ path('admin_modif_spectateur',  {'id': spectateur.id}) }}">
                    <button class="modifier" type="submit">Modifier</button>
                </a>
        {% endif %}
    </div>


{% endblock %}
